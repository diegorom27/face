{
    "builds": [
        {
            "src": "./src/index.js",
            "use": "@vercel/node",
            "routes": {
                "src": "/src/(.*)",
                "dest": "/"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "public/$1"
        }
    ]
}